#include<iostream>
#include<vector>
using namespace std;
int main(){
	
	
	
	int size;
	cout<<"Enter the Size of Vector : "<<endl; cin>>size;
	vector<int> vec(size);
	cout<<endl;
	cout<<endl;
	
for(int i=0; i<size; i++)
{
	cout<<"Enter the  "<< i <<"  index of vector : "; 
	cin>>vec[i];
}

cout<<endl;
cout<<endl;
cout<<"-----------------------------------------------"<<endl;
int target;
cout<<"Enter your Target : "; cin>>target; cout<<endl;
cout<<endl;
cout<<endl;
cout<<"----------------- OUTPUT  ---------------------"<<endl;
cout<<endl;

int x=-1;                                                                    //occerence
for(int i=1;i<size;i++)
{
	if(vec[i] == target)
	x=i;
}

if(x >= 0)
cout<<"Your Target's Last Exsistence is at  "<<x<<"  Index"<<endl;
else cout<<"Invalid Input...!";
}
